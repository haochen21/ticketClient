{
  "name"             : "ticketClient",
  "cwd"              : "/root/ticketClient",
  "script"           : "app.js",  
  "log_date_format"  : "YYYY-MM-DD HH:mm:ss",
  "error_file"       : "/root/ticketClient/log/stderr.log",
  "out_file"         : "/root/ticketClient/log/stdout.log",
  "pid_file"         : "/root/ticketClient/pids/node-geo-api.pid",
  "instances"        : 1, //or 0 => 'max'
  "min_uptime"       : "1m", // 200 seconds, defaults to 1000
  "max_restarts"     : 10, // defaults to 15
  "max_memory_restart": "200M", // 1 megabytes, e.g.: "2G", "10M", "100K", 1024 the default unit is byte.
  "watch"            : ["routes"],
  "merge_logs"       : true,
  "exec_interpreter" : "node",
  "exec_mode"        : "fork",
  "autorestart"      : true, // enable/disable automatic restart when an app crashes or exits
  "vizion"           : false  
}
